# data da entrega
dia = input()
entrega = int(input())
if entrega == 0:
    print('chega hoje!')
elif dia == "domingo":
    if entrega == 1:
        print('sera entregue segunda')
    elif entrega == 2:
        print('sera entregue terca')
    elif entrega == 3:
        print('sera entregue quarta')
    elif entrega == 4:
        print('sera entregue quinta')
    elif entrega == 5:
        print('sera entregue sexta')
    elif entrega == 6:
        print('sera entregue sabado')
